<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="Listing" table="listing">
      <meta attribute="class-description">
         This class contains the Listing detail. 
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="productID" column="productID" type="int"/>
      <property name="dealerID" column="dealerID" type="int"/>
      <property name="price" column="price" type="float"/>
      <property name="currency" column="currency" type="string"/>
      <property name="rating" column="rating" type="int"/>
      <property name="position" column="position" type="int"/>
      <property name="url" column="url" type="string"/>
      <property name="date" column="date" type="timestamp"/>
   </class>
</hibernate-mapping>